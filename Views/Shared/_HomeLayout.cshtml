        function SearchProducts() {

            var Items = $("#SearchItems").val();
            if (Items.trim() === "") {
                return false;
            }
            else {
                $.ajax({
                    type: "POST",
                    url: "/Products/SearchProducts",
                    data: { Query: Items },
                    success: function (response) {
                        console.log(response);
                    },
                    error: function (error) {
                        console.error(error);
                    }
                });
            }
        }
    </script>

