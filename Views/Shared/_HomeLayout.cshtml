        .navbar-nav .nav-item {
            position: relative;
            padding-bottom: 0px; /* Adjust as needed */
        }

            .navbar-nav .nav-item::after {
                content: '';
                position: absolute;
                left: 0px;
                bottom: -19px;
                width: 0;
                height: 2px; /* Height of the line */
                background-color: #ee3131; /* Color of the line (adjust as needed) */
                transition: width 0.3s ease; /* Transition effect */
            }


            .navbar-nav .nav-item:focus::after,
            .navbar-nav .nav-item:hover::after {
                width: 90%; /* Full width when focused or hovered */
            }

        .navbar-nav .nav-link::after {
            content: '';
            position: absolute;
            left: 0px;
            bottom: -19px;
            width: 0;
            height: 2px; /* Height of the line */
            background-color: #ee3131; /* Color of the line (adjust as needed) */
            transition: width 0.3s ease; /* Transition effect */
        }

        .navbar-nav .nav-link:focus::after,
        .navbar-nav .nav-link:hover::after {
            width: 90%; /* Full width when focused or hovered */
        }




